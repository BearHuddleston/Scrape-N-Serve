{
  "expo": {
    "name": "Scrape-N-Serve",
    "slug": "scrape-n-serve",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2196F3"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bearhuddleston.scrapenserve"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2196F3"
      },
      "package": "com.bearhuddleston.scrapenserve"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "backgroundColor": "#2196F3",
      "name": "Scrape-N-Serve",
      "shortName": "ScrapeNServe",
      "themeColor": "#2196F3"
    }
  }
}
